{
    "name": "wedevelopnl/silverstripe-elemental-grid",
    "description": "Elemental grid module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "element",
        "elemental",
        "content blocks",
        "grid"
    ],
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "silverstripe/admin": "^2.0@stable",
        "silverstripe/framework": "^5@stable",
        "silverstripe/vendor-plugin": "^2.0",
        "dnadesign/silverstripe-elemental": "^5.0",
        "wedevelopnl/silverstripe-media-field": "^4.0",
        "jonom/focuspoint": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "conflict": {
        "dnadesign/silverstripe-elemental": ">= 5.3.0"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/images"
        ]
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\ElementalGrid\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    }
}
